{
  "name": "vue-admin-antd",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint --fix --ext .js,.jsx,.vue src"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "ant-design-vue": "^1.4.4",
    "axios": "^0.18.1",
    "bluebird": "^3.5.5",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.1",
    "element-ui": "^2.12.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.14",
    "mavon-editor": "^2.7.5",
    "mockjs": "^1.0.1-beta3",
    "resize-detector": "^0.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "tinymce": "4.9.5",
    "v-charts": "^1.19.0",
    "vue": "^2.6.10",
    "vue-amap": "^0.5.10",
    "vue-baidu-map": "^0.21.22",
    "vue-grid-layout": "^2.3.4",
    "vue-highlightjs": "^1.3.3",
    "vue-infinite-scroll": "^2.0.2",
    "vue-kinesis": "^1.1.5",
    "vue-pdf": "^4.0.7",
    "vue-router": "^3.0.3",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vuedraggable": "^2.20.0",
    "vuex": "^3.0.1",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.9.0",
    "raw-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "eqeqeq": 1,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "max-len": [
        1,
        {
          "code": 140
        }
      ],
      "no-shadow": 0,
      "no-underscore-dangle": 0,
      "prefer-const": 1,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "import/extensions": 0,
      "import/no-duplicates": 0,
      "import/no-webpack-loader-syntax": 0,
      "no-param-reassign": 0,
      "import/no-extraneous-dependencies": 0,
      "no-debugger": 1,
      "import/no-unresolved": 0,
      "no-console": 1,
      "no-multi-spaces": 0,
      "quote-props": 0,
      "vue/valid-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "not ie <=8",
    "last 3 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
